ker\\src\\components\\Expenses\\ExpenseList.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\Expenses\\ExpenseItem.js",["57"],"\r\nimport React, {useState} from 'react';\r\nimport \"./ExpenseItem.css\";\r\nimport ExpenseDate from \"./ExpenseDate\";\r\nimport Card from '../UI/Card'\r\n\r\nfunction ExpenseItem(props) {\r\n  //deconstructed useState given the values of title and setTitle()\r\n  const [title, setTitle] = useState(props.title);\r\n\r\n  // handles the click event from the button\r\n  const clickHandler = ()=>{\r\n    let newTitle = \"This title was changed\"\r\n    //when this handler function happens, it will set off the setTitle() function and will reload the component\r\n    setTitle(newTitle)\r\n    console.log(\"i was clicked\")\r\n  }\r\n\r\n  return (\r\n    <Card className = \"expense-item\">\r\n      <ExpenseDate date={props.date}></ExpenseDate>\r\n      <div className=\"expense-item__description\">\r\n        <h2>{props.title}</h2>\r\n        <div className=\"expense-item__price\">{props.amount}\r\n        </div>\r\n        <button onClick={clickHandler}>click me!</button>\r\n      </div>\r\n    </Card>\r\n  );\r\n}\r\n\r\nexport default ExpenseItem;\r\n","C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\NewExpense\\ExpenseForm.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\Expenses\\ExpensesFilter.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\Expenses\\ExpensesChart.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\UI\\Card.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\Expenses\\ExpenseDate.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\Chart\\Chart.js",[],"C:\\Users\\leonyabo\\Documents\\React Course\\ExpenseTracker\\src\\components\\Chart\\ChartBar.js",[],{"ruleId":"58","severity